<Window x:Class="SetDemo.MainWindow"
        xmlns="http://schemas.microsoft.com/winfx/2006/xaml/presentation"
        xmlns:x="http://schemas.microsoft.com/winfx/2006/xaml"
        Title="MainWindow" Height="350" Width="625" Loaded="Window_Loaded">
    <Grid>
        <ComboBox Height="23" HorizontalAlignment="Left" Margin="14,11,0,0" Name="leftSet" VerticalAlignment="Top" Width="120" SelectionChanged="leftSet_SelectionChanged" />
        <ComboBox Height="23" HorizontalAlignment="Left" Margin="162,12,0,0" Name="operation" VerticalAlignment="Top" Width="120" />
        <ComboBox Height="23" HorizontalAlignment="Left" Margin="310,11,0,0" Name="rightSet" VerticalAlignment="Top" Width="120" SelectionChanged="rightSet_SelectionChanged" />
        <ListBox Height="217" HorizontalAlignment="Left" Margin="13,64,0,0" Name="leftMembers" VerticalAlignment="Top" Width="120" />
        <ListBox Height="217" HorizontalAlignment="Left" Margin="310,64,0,0" Name="rightMembers" VerticalAlignment="Top" Width="120" />
        <ListBox Height="217" HorizontalAlignment="Left" Margin="462,64,0,0" Name="resultSet" VerticalAlignment="Top" Width="120" />
        <Button Content="Evaluate" Height="23" HorizontalAlignment="Left" Margin="462,12,0,0" Name="evaluateButton" VerticalAlignment="Top" Width="75" Click="evaluateButton_Click" />
    </Grid>
</Window>
